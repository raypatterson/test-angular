<% content_for :head do %>

	<title><%= site_title %></title>
  <meta name="description" content="<%= site_description %>">
  <meta name="keywords" content="<%= site_keywords %>">
	<link rel="canonical" href="<%= site_url %>">

  <%= partial "library/partials/helpers/embed-icons" %>
  <%= partial "library/partials/helpers/social-tags" %>

<% end %>

<div class="top-level-container">

  <%= partial "project/modules/overlay/_partial" %>
  <%= partial "project/modules/header/_partial" %>
  <%= partial "project/modules/content/_partial" %>
  <%= partial "project/modules/footer/_partial" %>

</div>